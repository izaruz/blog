{% extends "admin-layout.twig" %}

{% block content %}
    <table class="table table-dark">
        <thead>
            <tr>
            <th scope="col">#</th>
            <th scope="col">Title</th>
            <th scope="col">Edit</th>
            <th scope="col">Delete</th>
            </tr>
        </thead>
        <tbody>
            {% for blogPost in blogPosts %}
            <tr>
                <td>{{ blogPost.id }}</td>
                <td>{{ blogPost.title }}</td>
                <td>Edit</td>
                <td>Delete</td>
            </tr>
            {% endfor %}
        </tbody>
    </table>
{% endblock %}